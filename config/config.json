{
  "$schema": "./schema.json",
  "title": "QRScout",
  "page_title": "4020 Scouting",
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": false,
      "fields": [
        {
          "title": "Scouter_Initials",
          "type": "text",
          "required": true,
          "rows": 1,
          "code": "scouter"
        },
        {
          "title": "Match_Number",
          "type": "text",
          "required": true,
          "rows": 1,
          "code": "match"
        },
        {
          "title": "Alliance_ID",
          "type": "select",
          "required": true,
          "code": "allianceId",
          "choices": {
            "r1": "Red 1",
            "r2": "Red 2",
            "r3": "Red 3",
            "b1": "Blue 1",
            "b2": "Blue 2",
            "b3": "Blue 3"
          },
          "defaultValue": "r1"
        },
        {
          "title": "Team_Number",
          "type": "text",
          "required": true,
          "rows": 1,
          "code": "teamNumber"
        },
        {
          "title": "Starting_Location",
          "type": "select",
          "choices": {
            "depot": "Depot",
            "middle": "Middle",
            "outpost": "Outpost",
            "noShow": "No Show"
          },
          "defaultValue": "noShow",
          "required": false,
          "code": "autoStart"
        }
      ]
    },
    {
      "name": "Auto",
      "fields": [
        {
          "code": "aa",
          "title": "Auto_Accuracy",
          "type": "select",
          "choices": {
            "terrible": "Terrrible",
            "bad": "Bad",
            "mid": "Mid",
            "great": "Great",
            "awesome": "Awesome"
          },
          "defaultValue": "terrible",
          "required": false
        },
        {
          "code": "ti",
          "title": "Type_Of_Intake",
          "type": "select",
          "choices": {
            "floor": "Floor",
            "outpost": "Outpost",
            "both": "Both"
          },
          "defaultValue": "floor",
          "required": false
        },
        {
          "code": "pm",
          "title": "Picked_Up_Mid",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-purple-500",
          "required": false
        },
        {
          "code": "po",
          "title": "Picked_Up_Outpost",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-purple-600",
          "required": false
