{
  "$schema": "../schema.json",
  "title": "QRScout",
  "page_title": "4020 Scouting",
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": false,
      "fields": [
        {
          "title": "Scouter_Initials",
          "type": "text",
          "required": true,
          "rows": 1,
          "code": "scouter"
        },
        {
          "title": "Match_Number",
          "type": "text",
          "required": true,
          "rows": 1,
          "code": "match"
        },
        {
          "title": "Alliance_ID",
          "type": "select",
          "required": true,
          "code": "allianceId",
          "choices": {
            "r1": "Red 1",
            "r2": "Red 2",
            "r3": "Red 3",
            "b1": "Blue 1",
            "b2": "Blue 2",
            "b3": "Blue 3"
          },
          "defaultValue": "r1"
        },
        {
          "title": "Team_Number",
          "type": "text",
          "required": true,
          "rows": 1,
          "code": "teamNumber"
        },
        {
          "title": "Starting_Location",
          "type": "select",
          "choices": {
            "processor": "Processor",
            "middle": "Middle",
            "opposite": "Opposite",
            "noShow": "No Show"
          },
          "defaultValue": "noshow",
          "required": false,
          "code": "autoStart"
        }
      ]
    },
    {
      "name": "Auto",
      "fields": [
        {
          "code": "lz",
          "title": "Leave_Zone",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "al1",
          "title": "Auto_Coral_L1",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-purple-300",
          "required": false
        },
        {
