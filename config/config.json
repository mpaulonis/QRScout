          "bgcolor": "bg-purple-500",
          "required": false
        },
        {
          "code": "al4",
          "title": "Auto_Coral_L4",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-purple-600",
          "required": false
        },
        {
          "code": "pa",
          "title": "Auto_Processed_Algae",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-emerald-500",
