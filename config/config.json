        {
          "code": "algaeIn",
          "title": "Algae_Intake_Type",
          "type": "select",
          "choices": {
            "reef": "Reef",
            "ground": "Ground",
            "both": "Both"
          },
          "defaultValue": "Reef",
          "required": false
        }
      ]
    },
    {
      "name": "Endgame Barge",
      "fields": [
        {
          "code": "tep",
          "title": "End_Position",
          "type": "select",
          "choices": {
            "none": "None",
            "parked": "Parked",
            "shallow": "Shallow (High)",
            "failShallow": "Shallow (High) failed",
            "deep": "Deep (Low)",
            "failDeep": "Deep (Low) failed"
          },
          "defaultValue": "None",
          "required": false
        },
        {
          "code": "cs",
          "title": "Climb_Speed",
          "type": "select",
          "choices": {
            "didNotClimb": "Did not Climb",
            "slow": "Slow (20-30 Sec.)",
            "med": "Med (10-20 Sec.)",
            "fast": "Fast (Under 10 Sec.)"
          },
          "defaultValue": "Slow",
          "required": false
        }
      ]
    },
    {
      "name": "Defense/Fouls",
      "fields": [
        {
          "code": "da",
          "title": "Defense_Amount",
          "type": "select",
          "choices": {
            "little": "None or very little",
            "moderate": "Moderate amount",
            "most": "Most of the time"
          },
          "defaultValue": "little",
          "required": false
        },
        {
          "code": "de",
          "title": "Defense_Effectiveness",
          "type": "select",
          "choices": {
            "no": "No defense played",
            "poor": "Poor",
            "ok": "OK",
            "great": "Excellent"
          },
          "defaultValue": "no",
          "required": false
        },
        {
          "code": "foul",
          "title": "Fouls",
          "type": "select",
          "choices": {
            "na": "Forgot to notice",
            "clean": "None noticed - clean",
            "1or2": "1 or 2 regular fouls",
            "multiple": "Tech foul or multiple regular",
            "aLot": "Fouling machine!"
          },
          "defaultValue": "na",
          "required": false
        },
        {
          "code": "toc",
          "title": "Touched_Opponent_Cage",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        }
      ]
    },
    {
      "name": "Evaluation",
      "fields": [
        {
          "code": "ds",
          "title": "Driver_Skill",
          "type": "select",
          "choices": {
            "na": "Not Noticed",
            "hindrance": "Hindrance",
            "poor": "Poor",
            "fair": "Fair",
            "good": "Mostly good",
            "great": "Very good"
          },
          "defaultValue": "na",
          "required": false
        },
        {
          "code": "rd",
          "title": "Robot_Died",
          "type": "select",
          "choices": {
            "no": "No",
            "yes": "Yes",
            "temporarily": "Temporarily"
          },
          "defaultValue": "No",
          "required": false
        },
        {
          "code": "to",
          "title": "Tipped_Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "co",
          "title": "Comments",
          "type": "text",
          "required": false,
          "rows": 3,
          "placeholder": "What was noteworthy or unusual about this performance? (defense, penalties, reliablity, etc.)"
        }
      ]
    }
  ]
}
