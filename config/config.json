        {
          "code": "pd",
          "title": "Picked_Up_Depot",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-purple-700",
          "required": false
        },
        {
          "code": "c",
          "title": "Climbed",
          "type": "select",
          "choices": {
            "yes": "Yes",
            "no": "No",
            "attempted": "Attempted"
          },
          "required": false
        },
        {
          "code": "ha",
          "title": "Had_Advantage",
          "type": "select",
          "choices": {
            "yes": "Yes",
            "no": "No"
          },
          "defaultValue": "no",
          "required": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "tol",
          "title": "Time_to_get_off_Latter",
          "type": "select",
          "choices": {
            "fast(1-3)": "Fast(1-3)",
            "med(4-6)": "Med(4-6)",
            "slow(7+)": "Slow(7+)"
          },
          "defaultValue": "Fast(1-3)",
          "required": false
        },
        {
          "code": "ut",
          "title": "Can_Go_Under_Trench",
          "type": "select",
          "choices": {
            "yes": "Yes",
            "no": "No"
          },
          "defaultValue": "no",
          "required": false
        },
        {
          "code": "or",
          "title": "Can_go_over_Ramp",
          "type": "select",
          "choices": {
            "yes": "Yes",
            "no": "No"
          },
          "defaultValue": "no",
          "required": false
        },
        {
          "code": "ta",
          "title": "Teleop_Accuracy",
          "type": "select",
          "choices": {
            "terrible": "Terrrible",
            "bad": "Bad",
            "mid": "Mid",
            "great": "Great",
            "awesome": "Awesome"
          },
          "defaultValue": "terrible",
          "required": false
        },
        {
          "code": "tr",
          "title": "Teleop_Range",
          "type": "range",
           "min": 0,
            "max": 10,
            "step": 1,
          "defaultValue": 5,
          "required": false
        },
        {
          "code": "ie",
          "title": "Intake_Effectiveness",
          "type": "select",
          "choices": {
            "terrible": "Terrrible",
            "bad": "Bad",
            "mid": "Mid",
            "great": "Great",
            "awesome": "Awesome"
          },
          "defaultValue": "terrible",
          "required": false
        },
        {
          "code": "afo",
          "title": "Attempt_to_Fill_Outpost",
          "type": "select",
          "choices": {
            "yes": "Yes",
            "no": "No"
          },
          "defaultValue": "no",
          "required": false
        },
        {
          "code": "hc",
          "title": "Hopper_Cycles",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-emerald-500",
          "required": false
        }
      ]
    },
    {
      "name": "Endgame Tower",
      "fields": [
        {
          "code": "ep",
          "title": "End_Position",
          "type": "select",
          "choices": {
            "notClimbed": "Not Climbed",
            "l1": "L1",
            "l2": "L2",
            "l3": "L3",
            "failed": "Failed"
          },
          "defaultValue": "notBlimbed",
          "required": false
        },
        {
          "code": "ttc",
          "title": "Time_When_Climb_Initiated",
          "type": "select",
          "choices": {
            "didNotClimb": "Did not Climb",
            "1-5": "1-5",
            "6-10": "6-10",
            "11-15": "11-15",
            "16-20": "16-20",
            "21-25": "21-25",
            "26-30": "26-30"
          },
          "defaultValue": "didNotClimb",
          "required": false
        }
      ]
    },
    {
      "name": "Defense/Fouls",
      "fields": [
        {
          "code": "da",
          "title": "Defense_Amount",
          "type": "select",
          "choices": {
            "little": "None or very little",
            "moderate": "Moderate amount",
            "most": "Most of the time"
          },
          "defaultValue": "little",
          "required": false
        },
        {
          "code": "de",
          "title": "Defense_Effectiveness",
          "type": "select",
          "choices": {
            "no": "No defense played",
            "poor": "Poor",
            "ok": "OK",
            "great": "Excellent"
          },
          "defaultValue": "no",
          "required": false
        },
        {
          "code": "foul",
          "title": "Fouls",
          "type": "select",
          "choices": {
            "na": "Forgot to notice",
            "clean": "None noticed - clean",
            "1or2": "1 or 2 regular fouls",
            "multiple": "Tech foul or multiple regular",
            "aLot": "Fouling machine!"
          },
          "defaultValue": "na",
          "required": false
        }
      ]
    },
    {
      "name": "Evaluation",
      "fields": [
        {
          "code": "hs",
          "title": "Average_Hopper_Size_When_Scoring",
          "type": "select",
          "choices": {
            "small(0-20)": "Small(0-20)",
            "med(21-40)": "Med(21-40)",
            "large(41+)": "Large(41+)"
          },
          "defaultValue": "small",
          "required": false
        },
        {
          "code": "ds",
          "title": "Driver_Skill",
          "type": "select",
          "choices": {
            "na": "Not Noticed",
            "hindrance": "Hindrance",
            "poor": "Poor",
            "fair": "Fair",
            "good": "Mostly good",
            "great": "Very good"
          },
          "defaultValue": "na",
          "required": false
        },
        {
          "code": "rd",
          "title": "Robot_Died",
          "type": "select",
          "choices": {
            "no": "No",
            "yes": "Yes",
            "temporarily": "Temporarily"
          },
          "defaultValue": "no",
          "required": false
        },
        {
          "code": "to",
          "title": "Tipped_Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "co",
          "title": "Comments",
          "type": "text",
          "required": false,
          "rows": 3,
          "placeholder": "What was noteworthy or unusual about this performance? (defense, penalties, reliablity, etc.)"
        }
      ]
    }
  ]
}
