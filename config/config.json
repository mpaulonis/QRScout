          "required": false
        },
        {
          "code": "tam",
          "title": "Teleop_Coral_L4",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "bgcolor": "bg-purple-600",
          "required": false
        },
        {
          "code": "coralIn",
          "title": "Coral_Intake_Type",
          "type": "select",
          "choices": {
            "source": "Source",
            "ground": "Ground",
            "both": "Both"
          },
          "defaultValue": "Source",
          "required": false
        },
        {
          "code": "litter",
          "title": "Coral_Litter",
          "type": "select",
          "choices": {
            "none": "None",
            "1to2": "1-2",
            "3to5": "3-5",
            "moreThan5": "More Than 5"
          },
          "defaultValue": "None",
          "required": false
        },
        {
          "code": "tap",
          "title": "Teleop_Processed_Algae",
